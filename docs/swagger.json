{
  "swagger": "2.0",
  "info": {
    "version": "0.0.1",
    "title": "user api"
  },
  "host": "127.0.0.1:8080",
  "schemes": [
    "http",
    "https"
  ],
  "paths": {
    "/user/login": {
      "post": {
        "operationId": "login",
        "summary": "登录",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "required": true,
            "description": "用户名",
            "schema": {
              "$ref": "#/definitions/UserLogin"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "正确返回",
            "examples": {
              "application/json": "{\n    \"data\": {\n        \"id\": \"102258\",\n        \"username\": \"test1\",\n        \"userType\": \"ABC\",\n        \"token\": \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJVc2VyIjp7IklEIjoxMDIyNTgsIlVzZXJOYW1lIjoidGVzdDEifSwiZXhwIjoxNTU3MzY4Njc5fQ.gI5Zihc_ZZC4v3eronIFL0pTUl7BitBlUjLoyTtZt4U\",\n        \"err\": null\n    }\n}"
            }
          },
          "400": {
            "description": "错误返回",
            "examples": {
              "application/json": "{\n    \"desc\": \"密码错误\",\n    \"err\": \"InvalidParamError\"\n}"
            }
          },
          "404": {
            "description": "错误返回",
            "examples": {
              "application/json": "{\n    \"desc\": \"用户不存在\",\n    \"err\": \"NotFoundError\"\n}"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "UserLogin": {
      "type": "object",
      "allOf": [
        {
          "required": ["username"],
          "properties": {
            "username": {
              "type": "string"
            },
            "password": {
              "type": "string"
            }
          }
        }
      ]
    }
  }
}